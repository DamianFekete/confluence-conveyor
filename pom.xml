<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	
    <parent>
        <groupId>com.atlassian.pom</groupId>
        <artifactId>atlassian-contrib-pom</artifactId>
        <version>5</version>
    </parent>
	
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.randombits.confluence</groupId>
	<artifactId>confluence-conveyor</artifactId>
	<name>Confluence Conveyor</name>
	<version>0.4.2-SNAPSHOT</version>
	
	<url>http://www.customware.net/repository/display/AtlassianPlugins/Confluence+Conveyor+Library</url>
	
	<properties>
		<confluence.version>2.6.0</confluence.version>
	</properties>
	
	<scm>
		<connection>scm:svn:http://svn.atlassian.com/svn/public/contrib/confluence/libraries/org.randombits.confluence/confluence-conveyor/trunk</connection>
		<developerConnection>scm:svn:https://svn.atlassian.com/svn/public/contrib/confluence/libraries/org.randombits.confluence/confluence-conveyor/trunk</developerConnection>
		<url>http://svn.atlassian.com/fisheye/browse/public/contrib/confluence/libraries/org.randombits.confluence/confluence-conveyor/trunk</url>
	</scm>
	
	<dependencies>
		<!-- dependency>
		<groupId>org.randombits.confluence</groupId>
		<artifactId>confluence-intercom</artifactId>
		<version>2.0.3-SNAPSHOT</version>
		</dependency -->
		<!-- Confluence Dependencies -->
		<dependency>
			<groupId>com.atlassian.confluence</groupId>
			<artifactId>confluence</artifactId>
			<version>${confluence.version}</version>
			<scope>provided</scope>
			<exclusions>
				<exclusion>
					<groupId>opensymphony</groupId>
					<artifactId>pell-multipart</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
	
	<repositories>
		<repository>
			<id>central-contrib</id>
			<url>https://maven.atlassian.com/repository/public</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
		<repository>
			<id>central-contrib</id>
			<url>https://maven.atlassian.com/contrib</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>
	
</project>